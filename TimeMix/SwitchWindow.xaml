<Window x:Class="TimeMix.SwitchWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeMix"
        mc:Ignorable="d"
        Title="SwitchWindow" Height="200" Width="40" Closed="Window_Closed" WindowStyle="None" MouseLeftButtonDown="Window_MouseLeftButtonDown" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Image HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="40" Source="/Resources/Switch/Time.png"/>
            <Image HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="40" Source="/Resources/Switch/ClassTable.png"/>
            <Image HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="40" Source="/Resources/Switch/TimeTable.png"/>
            <Image HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="40" Source="/Resources/Switch/Setting.png" />
            <Image HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="40" Source="/Resources/Switch/Other.png"/>
        </StackPanel>
    </Grid>
</Window>
