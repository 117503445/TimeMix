            <Label x:Name="Lbl5" Content="电教" HorizontalContentAlignment="Center"  Padding="1"/>
            <Label x:Name="Lbl6" Content="委员" HorizontalContentAlignment="Center"  Padding="1"/>
            <Label x:Name="Lbl7" Content="给他" HorizontalContentAlignment="Center"  Padding="1"/>
            <Label x:Name="Lbl8" Content="看看" HorizontalContentAlignment="Center"  Padding="1"/>
